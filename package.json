{
  "name": "pse-homepage-ui",
  "version": "1.0.0",
  "description": "FirstNet Local Control",
  "main": "index.jsx",
  "scripts": {
    "copyassets": "copyfiles ./apple-touch-icon.png ./images/**/* ./fonts/**/* ./maps/* ./build/",
    "build": "rm -rf build & npm run copyassets & webpack -p",
    "start": "npm run copyassets & webpack-dev-server --hot --open --env=dev --progress",
    "test": "jest --coverage",
    "testwatch": "jest --watch"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.0",
    "bootstrap": "^3.3.7",
    "date-fns": "^1.29.0",
    "fn-joyride-ui": "git+ssh://git@us.tools.publicis.sapient.com/fnapi/fn-joyride-ui.git#v2.1.0",
    "fn-common-ui": "git+ssh://git@us.tools.publicis.sapient.com/fnapi/common-ui.git#v1.5.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "node-sass": "^4.5.3",
    "process-nextick-args": "^1.0.7",
    "react": "^16.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-router-dom": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^config$": "<rootDir>/config/dev.endpoints.js"
    },
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "automock": true,
    "setupFiles": [
      "./jestsetup.js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "prop-types",
      "react-router-dom",
      "lodash",
      "axios",
      "date-fns",
      "history"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.0.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "json-server": "^0.12.0",
    "lodash-webpack-plugin": "^0.11.4",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3"
  }
}

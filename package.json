{
  "name": "pse-homepage-ui",
  "version": "1.0.0",
  "description": "FirstNet PSE Homepage",
  "main": "index.jsx",
  "scripts": {
    "copyassets": "copyfiles ./images/**/* ./fonts/**/* ./maps/* ./build/",
    "build": "rm -rf build & npm run copyassets & webpack -p --progress",
    "start": "npm run copyassets & webpack-dev-server --open",
    "test": "jest --coverage"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^0.22.0",
    "jquery": "^3.2.1",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.3",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "endpoints$": "<rootDir>/cfg/endpoints.js"
    },
    "automock": true,
    "setupFiles": [
      "./jestsetup.js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "prop-types",
      "react-router-dom"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^6.10.3",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-cli": "^15.1.1",
    "json-server": "^0.9.6",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}

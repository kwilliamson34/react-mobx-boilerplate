<!doctype html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<title>FirstNet Local Control</title>
	<meta name="application-name" content="LocalControl" />
	<meta name="apple-mobile-web-app-title" content="FirstNet LocalControl" />
	<meta property="og:title" content="FirstNet Local Control" />
	<meta property="og:site_name" content="FirstNet Local Control" />
	<meta property="og:type" content="website" />
	<meta property="og:locale" content="en_US" />
	<meta property="og:description" content="Local control is a key feature of the FirstNet solution, providing a powerful capability to optimize the user experience through a single convenient home page location." />
	<meta property="og:image" content="https://localcontrol.firstnet.att.com/apple-touch-icon.png" />

	<link rel="icon" type="image/png" sizes="192x192" href="/images/firstnetlogo-192x192.png" />
	<link rel="icon" type="image/png" sizes="128x128" href="/images/firstnetlogo-128x128.png" />
	<link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png" />
	<link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png" />

</head>

<body>
	<!--These spans are outside of the react app -->
	<span role="alert" class="sr-only" id="global-a11y-alert-announcer"></span>
	<span role="status" class="sr-only" id="global-a11y-status-announcer"></span>
	<div id="app">
		<!--loading animation initial load-->
		<style type="text/css">
			body * {
				margin: 0;
				padding: 0;
				box-sizing: border-box;
			}

			.fn-loading {
				text-align: center;
				padding-top: 80px;
			}

			.fn-loading-logo {
				display: inline-block;
				margin: 0 auto 50px;
				width: 200px;
			}

			.fn-loading-logo img {
				max-width: 100%;
			}

			.fn-loading-text {
				text-transform: uppercase;
				font-family: 'Roboto', 'Helvetica Neue', sans-serif;
				font-weight: bold;
				font-size: 14px;
			}

			.fn-loading-circle {
				margin: 15px auto;
				width: 40px;
				height: 40px;
				position: relative;
			}

			.fn-loading-circle>div {
				width: 100%;
				height: 100%;
				position: absolute;
				left: 0;
				top: 0;
			}

			.fn-loading-circle>.c:before {
				content: '';
				display: block;
				margin: 0 auto;
				width: 15%;
				height: 15%;
				background-color: #3a3a3a;
				border-radius: 100%;
				-webkit-animation: circleBounceDelay 1.2s infinite ease-in-out both;
				animation: circleBounceDelay 1.2s infinite ease-in-out both;
			}

			.c2 {
				-webkit-transform: rotate(30deg);
				-ms-transform: rotate(30deg);
				transform: rotate(30deg);
			}

			.c3 {
				-webkit-transform: rotate(60deg);
				-ms-transform: rotate(60deg);
				transform: rotate(60deg);
			}

			.c4 {
				-webkit-transform: rotate(90deg);
				-ms-transform: rotate(90deg);
				transform: rotate(90deg);
			}

			.c5 {
				-webkit-transform: rotate(120deg);
				-ms-transform: rotate(120deg);
				transform: rotate(120deg);
			}

			.c6 {
				-webkit-transform: rotate(150deg);
				-ms-transform: rotate(150deg);
				transform: rotate(150deg);
			}

			.c7 {
				-webkit-transform: rotate(180deg);
				-ms-transform: rotate(180deg);
				transform: rotate(180deg);
			}

			.c8 {
				-webkit-transform: rotate(210deg);
				-ms-transform: rotate(210deg);
				transform: rotate(210deg);
			}

			.c9 {
				-webkit-transform: rotate(240deg);
				-ms-transform: rotate(240deg);
				transform: rotate(240deg);
			}

			.c10 {
				-webkit-transform: rotate(270deg);
				-ms-transform: rotate(270deg);
				transform: rotate(270deg);
			}

			.c11 {
				-webkit-transform: rotate(300deg);
				-ms-transform: rotate(300deg);
				transform: rotate(300deg);
			}

			.c12 {
				-webkit-transform: rotate(330deg);
				-ms-transform: rotate(330deg);
				transform: rotate(330deg);
			}

			.c2.c:before {
				-webkit-animation-delay: -1.1s;
				animation-delay: -1.1s;
			}

			.c3.c:before {
				-webkit-animation-delay: -1s;
				animation-delay: -1s;
			}

			.c4.c:before {
				-webkit-animation-delay: -0.9s;
				animation-delay: -0.9s;
			}

			.c5.c:before {
				-webkit-animation-delay: -0.8s;
				animation-delay: -0.8s;
			}

			.c6.c:before {
				-webkit-animation-delay: -0.7s;
				animation-delay: -0.7s;
			}

			.c7.c:before {
				-webkit-animation-delay: -0.6s;
				animation-delay: -0.6s;
			}

			.c8.c:before {
				-webkit-animation-delay: -0.5s;
				animation-delay: -0.5s;
			}

			.c9.c:before {
				-webkit-animation-delay: -0.4s;
				animation-delay: -0.4s;
			}

			.c10.c:before {
				-webkit-animation-delay: -0.3s;
				animation-delay: -0.3s;
			}

			.c11.c:before {
				-webkit-animation-delay: -0.2s;
				animation-delay: -0.2s;
			}

			.c12.c:before {
				-webkit-animation-delay: -0.1s;
				animation-delay: -0.1s;
			}

			@-webkit-keyframes circleBounceDelay {
				0%,
				80%,
				100% {
					-webkit-transform: scale(0);
					transform: scale(0);
				}
				40% {
					-webkit-transform: scale(1);
					transform: scale(1);
				}
			}

			@keyframes circleBounceDelay {
				0%,
				80%,
				100% {
					-webkit-transform: scale(0);
					transform: scale(0);
				}
				40% {
					-webkit-transform: scale(1);
					transform: scale(1);
				}
			}

			@media only screen and (min-width: 768px) {
				.fn-loading {
					padding-top: 135px
				}
			}
		</style>
		<div class="fn-loading">
			<div class="fn-loading-logo">
				<img src="/images/firstnet-logo.svg" width="173" height="51" alt="" aria-hidden="true" />
			</div>
			<h1 class="fn-loading-text">Loading</h1>
			<div class="fn-loading-circle">
				<div class="c1 c"></div>
				<div class="c2 c"></div>
				<div class="c3 c"></div>
				<div class="c4 c"></div>
				<div class="c5 c"></div>
				<div class="c6 c"></div>
				<div class="c7 c"></div>
				<div class="c8 c"></div>
				<div class="c9 c"></div>
				<div class="c10 c"></div>
				<div class="c11 c"></div>
				<div class="c12 c"></div>
			</div>
		</div>
	</div>
	<div id="portal"></div>
</body>
</html>
